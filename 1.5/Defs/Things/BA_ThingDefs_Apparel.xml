<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <ThingDef Abstract="True" Name="BA_Apparel_Base" ParentName="ApparelMakeableBase">
    <thingCategories Inherit="False">
      <li>BA_ThingCategory_Apparel</li>
    </thingCategories>

    <apparel Inherit="False">
      <useWornGraphicMask>true</useWornGraphicMask>
      <tags>
        <li>BA_ApparelTag</li>
        <li>BA_ApparelTag_Apparel</li>
      </tags>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>OnSkin</li>
      </layers>
    </apparel>

    <smeltable>false</smeltable>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>1600</WorkToMake>
      <Mass>0.5</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.20</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.08</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.2</SocialImpact>
      <TradePriceImprovement>0.2</TradePriceImprovement>
      <ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">0.1</ToxicEnvironmentResistance>
    </equippedStatOffsets>

    <costStuffCount>40</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>

    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
      <displayPriority>265</displayPriority>
    </recipeMaker>

    <colorGenerator Class="ColorGenerator_White" />
  </ThingDef>

